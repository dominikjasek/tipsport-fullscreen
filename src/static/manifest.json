{
  "name": "Tipsport Fullscreen",
  "description": "Zobrazení streamu na Tipsportu v režimu Fullscreen",
  "version": "1.1",
  "manifest_version": 3,
  "icons": {
    "128": "tipsport-logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.tipsport.cz/*"
      ],
      "js": [
        "fullscreen.js"
      ]
    }
  ]
}
