{
  "name": "Tipsport Fullscreen",
  "description": "Zobrazení streamu na Tipsportu v režimu Fullscreen",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "tipsport-logo-48.png",
    "96": "tipsport-logo-96.png",
    "128": "tipsport-logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.tipsport.cz/live/*"
      ],
      "js": [
        "fullscreen.js"
      ]
    }
  ]
}
